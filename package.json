{
    "name": "typeorm-extension",
    "version": "2.1.10",
    "description": "Typeorm extension to create/drop database, simple seeding data sets, ...",
    "author": {
        "name": "Peter Placzek",
        "email": "contact@tada5hi.net",
        "url": "https://github.com/tada5hi"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/tada5hi/typeorm-extension.git"
    },
    "main": "dist/index.js",
    "files": [
        "dist/"
    ],
    "scripts": {
        "build": "rm -rf ./dist && tsc",
        "build:watch": "npm run build -- --watch",
        "test": "cross-env NODE_ENV=test jest --config ./test/jest.config.js",
        "test:coverage": "cross-env NODE_ENV=test jest --config ./test/jest.config.js --coverage",
        "lint": "eslint --ext .js,.vue,.ts ./src",
        "lint:fix": "npm run lint -- --fix",
        "docs:dev": "vitepress dev docs --temp .temp",
        "docs:build": "vitepress build docs --temp .temp",
        "docs:help": "vitepress --help",
        "prepublishOnly": "npm run build"
    },
    "keywords": [
        "database",
        "create",
        "drop",
        "api",
        "json-api",
        "jsonapi",
        "seeder",
        "seeding",
        "cli"
    ],
    "bin": {
        "typeorm-extension": "dist/cli/index.js"
    },
    "license": "MIT",
    "dependencies": {
        "@faker-js/faker": "^7.5.0",
        "locter": "^0.2.2",
        "rapiq": "^0.0.4",
        "reflect-metadata": "^0.1.13",
        "yargs": "^17.5.1"
    },
    "peerDependencies": {
        "typeorm": "~0.3.0"
    },
    "devDependencies": {
        "@tada5hi/eslint-config-typescript": "^1.0.7",
        "@types/glob": "^8.0.0",
        "@types/jest": "^27.5.0",
        "@types/node": "^18.7.21",
        "@types/yargs": "^17.0.13",
        "better-sqlite3": "^7.6.2",
        "coveralls": "^3.1.0",
        "cross-env": "^7.0.3",
        "eslint": "^8.24.0",
        "jest": "^27.5.1",
        "np": "^7.6.2",
        "ts-jest": "^27.1.4",
        "ts-node": "^10.9.1",
        "typeorm": "^0.3.10",
        "typescript": "^4.8.3",
        "vitepress": "^1.0.0-alpha.16",
        "vue": "^3.2.39"
    }
}
